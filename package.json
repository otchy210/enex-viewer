{
  "name": "enex-viewer",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/api",
    "apps/web"
  ],
  "scripts": {
    "dev": "npm run dev:api & npm run dev:web",
    "dev:api": "npm run dev -w apps/api",
    "dev:web": "npm run dev -w apps/web",
    "build": "npm run build:api && npm run build:web",
    "build:api": "npm run build -w apps/api",
    "build:web": "npm run build -w apps/web",
    "typecheck": "npm run typecheck:api && npm run typecheck:web",
    "typecheck:api": "npm run typecheck -w apps/api",
    "typecheck:web": "npm run typecheck -w apps/web",
    "test": "npm run test:api && npm run test:web",
    "test:api": "npm run test -w apps/api",
    "test:web": "npm run test -w apps/web"
  }
}
